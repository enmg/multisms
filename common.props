<?xml version="1.0" encoding="UTF-8"?>
<Project>
	<PropertyGroup>
		<Version>1.0.1</Version>
		<NoWarn>$(NoWarn);CS1591</NoWarn>
		<RepositoryUrl>https://github.com/mustafagenc/multisms</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>sms, sms provider, netgsm</PackageTags>
		<Authors>Mustafa Genç</Authors>
		<ProductName>MultiSMS</ProductName>
		<Copyright>Copyright © 2023</Copyright>
		<MaintainerEmail>eposta@mustafagenc.info</MaintainerEmail>
		<PackageIcon>icon.png</PackageIcon>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)' == 'Release' ">
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<DebugType>none</DebugType>
		<DebugSymbols>false</DebugSymbols>
	</PropertyGroup>
	<ItemGroup>
		<Content Include="$(MSBuildThisFileDirectory)icon.png" Pack="true" PackagePath="/">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
	</ItemGroup>
</Project>